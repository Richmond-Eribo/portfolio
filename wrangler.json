{
  "name": "portfolio",
  "main": "./workers/app.ts",
  "compatibility_date": "2024-11-11",
  "compatibility_flags": ["nodejs_als", "nodejs_compat"],
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist/assets",
    "html_handling": "drop-trailing-slash",
    "not_found_handling": "404-page"
  },

  "observability": {
    "logs": {
      "enabled": true,
      "head_sampling_rate": 1,
      "invocation_logs": true
    }
  }
}
